{
  "version": 2,
  "name": "Franco Silva",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "use": "@vercel/static-build",
      "src": "package.json"
    }
  ],
  "routes": [
    {
      "src": "(.*).js",
      "dest": "/$1.js"
    }
  ]
}